<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.personalkasappv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_add"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.android.personalkasappv2.Splashscreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.personalkasappv2.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.android.personalkasappv2.AddActivity" />
        <activity android:name="com.example.android.personalkasappv2.EditActivity" />
        <activity android:name="com.example.android.personalkasappv2.FilterActivity"></activity>
    </application>

</manifest>